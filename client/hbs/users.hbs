{{>partials/head}}
{{> partials/header}}
<div class="space"></div>
<section class="Users uk-container">
    <h1 class="title">Crear usuario</h1>
    <div class="content">
        <form class="createUser">
            <div class="container">
                <label for="name">Nombre</label>
                <input type="text" id="name" name="name">
            </div>
            <br>
            <div class="container">
                <label for="lastname">Apellido</label>
                <input type="lastname" id="lastname" name="lastname">
            </div>
            <br>
            <div class="container">
                <label for="email">Email</label>
                <input type="email" id="email" name="email">
            </div>
            <br>
            <div class="container">
                <label for="adminOrUser">Perfil</label>
                <input type="text" id="adminOrUser" name="adminOrUser">
            </div>
            <br>
            <div class="container">
                <label for="passwordd">Contraseña</label>
                <input type="password" id="passwordd" name="passwordd">
            </div>
            <br>
            <div class="container">
                <label for="password">Repetir contraseña</label>
                <input type="password" id="password" name="password">
            </div>
            <br>
            <button type="submit" id="create">Crear</button>
        </form>
    </div>
</section>
{{> partials/footer}}
{{>partials/foot}}